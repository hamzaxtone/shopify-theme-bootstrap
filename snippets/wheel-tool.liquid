{% render 'wheel-tool-css' %}
<style>
  div#tool-imgg {
    width: 505px;
    height: 500px;
    margin-top:110px;
}
  .tool-img img{
  	width:505px;
    height:505px;
    left: 0;
    top: 0;
  }
  .dskd .tool-img,.dskd .tool-img .imagesss{
  	position:relative
  }
  .buttons{
/*     position: absolute;
    left: 0;
    width: 100%;
    bottom: 450px;
    display: flex;
    flex-flow: row; */
        display: flex;
    flex-flow: row;
    position: relative;
    margin-bottom: 40px;
  }
  
  .buttons button,.buttons a {
    margin-left: 5px;
    outline: none;
    box-shadow: none;
    border: none;
    color: #000;
    background-color: orange;
    min-width: 110px;
    font-weight: 600;
    padding: 10px;
    width: auto;
    display: block;
    text-decoration:none;
  }
  .canvass{
  	opacity:0;
    visibility:hidden;
    position:absolute;
    left:0;
    top:0;
    z-index:-99;
  }
  
  .actions ul.designs {
    display: flex;
    list-style: none;
    flex-flow:wrap;
    padding:0;
}

  .actions ul.designs li{
  	position:relative;
    margin-left:5px;
    margin-right:5px;
  }
  .actions ul.designs li i {
    width: 25px;
    height: 25px;
    position: absolute;
    top: 0;
    border: 1px solid;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    border-radius: 50%;
    background-color: transparent;
    color: #000;
}

.actions ul.designs li i:hover {
    background-color: #000;
    color: #fff;
}
  
.actions ul.designs li img {
    width: 70px;
  margin-top:10px;
}
</style>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.slim.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/tinyColorPicker/1.1.1/jqColorPicker.min.js"></script>



<div class="fui flexbox dskd container">
  
	<div class="frame dashboard">
      <div class="img_loading"><span>Loading</span></div>
      <div class="buttons">
    <button id="reset">Reset</button>
    <button id="addToDesign">Add to My Design</button>
<!--     <button id="download">Download</button> -->
        <a class="download" id="download" target="_blank">Download</a> 
        
</div>
	<div class="workspace">
      
		
      {%- capture contentForQuerystring -%}{{ content_for_header }}{%- endcapture -%}
{%- assign pageUrl = contentForQuerystring | split:'"pageurl":"' | last | split:'"' | first | split:'.myshopify.com' | last |
   replace:'\/','/' | 
   replace:'%20',' ' | 
   replace:'\u0026','&'
-%}

{%- assign pageQuerystring = pageUrl | split:'=' | last -%}
{%- assign parr = pageUrl | split:'?' | last -%}



{%- if parr contains "slug" %}

    {%- assign para = parr | split:'&' | first -%}
    {%- assign proSLug = para | split:'=' | last -%}

    {% assign product = all_products[proSLug] %}
          <input type="hidden" class="keyCount" value="1" />
    {% if product == blank %}
        <h1>No configurator wheel found</h1>
          <input type="hidden" class="key" value="key-test" />

    {% else %}
          <input type="hidden" class="key" value="key-{{product.id}}" />


          <div class="fui flexbox title">
                <div class="fui flexbox plate">
                    <a class="title-link" href="/products/{{ product.handle }}">
                        <div class="fui flexbox " style="place-content: center; align-items: center;">
                            <svg width="10" height="10" class="all-wheels-icon" xmlns="http://www.w3.org/2000/svg" viewBox="-5 0 25.618 25.618" style="margin-right: 2px;">
                                <g data-name="Layer 77" transform="translate(-8.191 -3.191)">
                                    <g data-name="Group 35">
                                        <rect data-name="Rectangle 38" width="18.141" height="3.947" rx="1.954" transform="translate(8.191 16.019) rotate(-45)" fill="#161615" style="fill: white;"></rect>
                                    </g>
                                    <g data-name="Group 36">
                                        <rect data-name="Rectangle 39" width="3.947" height="18.141" rx="1.954" transform="translate(8.191 15.981) rotate(-45)" fill="#161615" style="fill: white;"></rect>
                                    </g>
                                </g>
                            </svg>Back</div>
                    </a>
                    <div class="fui flexbox " style="align-items: center;"><span class="wheel-name">{{ product.title }}</span><span class="wheel-series"></span></div>
                </div>
            </div>

    {% endif %}
{% else %}      
      <div class="fui flexbox title">
                <div class="fui flexbox plate">
                    
                    <div class="fui flexbox " style="align-items: center;"><span class="wheel-name">General Tool</span><span class="wheel-series"></span></div>
                </div>
            </div>
      
{% endif %}
      
      
      
      
      
      
      
      
