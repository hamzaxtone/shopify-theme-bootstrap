body,h1,h2,h3,h4,h5,h6,p,a,button,input{
  font-family: 'Open Sans';
}

ul#navbar-nav li a {
    font-size: 14px;
    text-transform: uppercase;
    color: #fff;
    font-family: 'Open Sans';
    font-weight: bold;
}
ul#navbar-icons-nav li a {
    color: #fff;
}
.carousel .carousel-caption {
    top: 18%;
    bottom: unset;
}
.carousel .carousel-caption h3 {
    font-size: 105px;
    font-family: 'Open Sans';
    font-weight: bold;
    line-height: 115px;
  text-transform: uppercase;
}
.carousel .carousel-caption p {
    font-size: 20px;
    line-height: 20px;
    font-weight: 400;
    font-family: 'Open Sans';
}
.carousel .carousel-caption .btn {
   	font-size: 16px;
    line-height: 28px;
    font-weight: 700;
    font-family: 'Open Sans';
    padding: 13px 15px;
    border: 1px solid #fff;
    color: #fff;
    min-width: 220px;
    border-radius: 5px;
}